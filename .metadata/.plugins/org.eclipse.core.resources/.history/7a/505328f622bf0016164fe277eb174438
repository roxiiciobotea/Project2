package serviceComponents;

public class Event {
	private String type;
	private Filter filter = null;

	public Event(String type) {
		this.type = type;
	}

	public String getType() {
		return this.type;
	}

	public void setFilter(Filter f) {
		this.filter = f;
	}

	public boolean hasFilter() {
		if (this.filter == null)
			return false;
		return true;
	}
}
